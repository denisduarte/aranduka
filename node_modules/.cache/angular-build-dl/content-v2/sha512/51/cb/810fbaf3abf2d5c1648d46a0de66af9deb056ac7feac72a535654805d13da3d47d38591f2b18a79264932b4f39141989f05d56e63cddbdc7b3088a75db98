      var BookListComponent = /*#__PURE__*/function () {
        function BookListComponent(router, route, bookService, search, userService) {
          _classCallCheck(this, BookListComponent);

          this.router = router;
          this.route = route;
          this.bookService = bookService;
          this.search = search;
          this.userService = userService;
          this.offset = {
            has: false,
            value: 0
          };
          this.lefts = [];
          this.bookList = [];
          this.page = 0;
          this.booksPerPage = 30;
          this.filterTag = [];
          this.filterSearch = '';
          this.sortField = 'createdAt';
          this.sortDirection = 1;
        }

        _createClass(BookListComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            var _this36 = this;

            //this.getBooks();
            this.getScreenSize();
            this.search.searchInputUpdated.subscribe(function (searchInputValue) {
              _this36.filterSearch = searchInputValue;

              _this36.getBooks(_this36.filterSearch, _this36.filterTag, 0, _this36.booksPerPage, false, _this36.sortField, _this36.sortDirection);
            });
            this.userSubscription = this.userService.user.subscribe(function (user) {
              //this.isAuthenticated = !!user;
              _this36.user = user;
            });
            this.bookListSubscription = this.bookService.bookList.subscribe(function (bookList) {
              _this36.bookList = bookList;
            });
            this.routeSubscription = this.route.queryParams.subscribe(function (params) {
              if (params.tag) _this36.filterByTag(params.tag);
            });
            this.filterSearch = this.search.searchInputValue;
            this.search.searchInputUpdated.emit(this.filterSearch);
          }
        }, {
          key: "sortOrder",
          value: function sortOrder(field) {
            //if its the same field, reverse order
            if (field == this.sortField) {
              this.sortDirection = -1 * this.sortDirection;
            } // otherwise, set to default
            else {
                this.sortDirection = 1;
              }

            this.sortField = field;
            this.getBooks(this.filterSearch, this.filterTag, 0, this.booksPerPage, false, this.sortField, this.sortDirection);
          }
        }, {
          key: "getBooks",
          value: function getBooks() {
            var searchInput = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : '';
            var tagInput = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : [];
            var page = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 0;
            var booksPerPage = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : 30;
            var appendResults = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : false;
            var sortField = arguments.length > 5 && arguments[5] !== undefined ? arguments[5] : "createdAt";
            var sortDirection = arguments.length > 6 && arguments[6] !== undefined ? arguments[6] : -1;
            this.bookService.getBookList(searchInput, tagInput, page, booksPerPage, appendResults, sortField, sortDirection);
          }
        }, {
          key: "calculateLefts",
          value: function calculateLefts(Ul, lefts) {
            var books = Ul.nativeElement.childNodes;

            for (var i = 0; i < books.length - 1; i++) {
              var item = books[i];
              lefts[i] = item.getBoundingClientRect().left;
            }
          }
        }, {
          key: "ngAfterViewInit",
          value: function ngAfterViewInit() {
            setTimeout(this.calculateLefts.bind(null, this.bookUl, this.lefts), 1000);
          }
        }, {
          key: "getScreenSize",
          value: function getScreenSize() {
            var win = window,
                d = document,
                e = d.documentElement,
                g = d.getElementsByTagName('body')[0],
                w = win.innerWidth,
                h = win.innerHeight;
            this.screenHeight = h;
            this.screenWidth = w;
          } //function that calculates the offset of the floating box to prevent
          // it to going off-screen

        }, {
          key: "mouseEnter",
          value: function mouseEnter(i) {
            console.log('enter', i);
            this.getScreenSize();
            this.calculateLefts(this.bookUl, this.lefts);
            var bookDiv = this.bookDivs.toArray()[i]; //Getting the margin and padding of the container div

            var container_div = bookDiv.nativeElement.closest('.first-container');
            var containerMargin = parseFloat(window.getComputedStyle(container_div, null).getPropertyValue('margin-right'));
            var containerPadding = parseFloat(window.getComputedStyle(container_div, null).getPropertyValue('padding-right')); //The original size of the div

            var divWidth = bookDiv.nativeElement.offsetWidth; //The % of increase of the div size given by the transformation

            var container_li = bookDiv.nativeElement.closest('li');
            var transform = window.getComputedStyle(container_li, null).getPropertyValue('transform');
            var matrixValues = transform.match(/matrix.*\((.+)\)/)[1].split(', ');
            var scale = parseFloat(matrixValues[0]); /////temp
            //console.log(scale);

            switch (true) {
              case this.screenWidth >= 768:
                {
                  scale = 1.5;
                  break;
                }

              case this.screenWidth >= 576:
                {
                  scale = 1.1;
                  break;
                }
            } //The original position of the div


            var myOriginalLeft = this.lefts[i];
            /*
            var rect = bookDiv.nativeElement.getBoundingClientRect();
            var divLeft = rect.left
            var divPaddingL = parseFloat(window.getComputedStyle(bookDiv.nativeElement, null)
                                        .getPropertyValue('padding-left'));
            var divPaddingR = parseFloat(window.getComputedStyle(bookDiv.nativeElement, null)
                                        .getPropertyValue('padding-right'));
                                */

            if (!this.offset.has) {
              this.offset.has = true;
              this.offset.value = (this.screenWidth - myOriginalLeft - divWidth * scale - (containerMargin + containerPadding + 7)) / scale;
            } //console.log(this.offset.value);


            if (this.offset.value < 0) {
              //setTimeout(() => {
              bookDiv.nativeElement.parentNode.parentNode.style.setProperty('transform', 'scale(1) translateY(20px) translateX(' + this.offset.value + 'px)'); //}, 500);
            }
          }
        }, {
          key: "mouseOut",
          value: function mouseOut(i) {
            console.log('out', i);
            var bookDiv = this.bookDivs.toArray()[i];

            if (this.offset.has) {
              //setTimeout(() => {
              bookDiv.nativeElement.parentNode.parentNode.style.setProperty('transform', '');
              this.offset.has = false;
              this.offset.value = 0; //  }, 500);
            }
          }
        }, {
          key: "filterByTag",
          value: function filterByTag(tag) {
            console.log('filterByTag');

            if (this.filterTag.indexOf(tag) === -1) {
              console.log('entrei');
              this.filterTag.push(tag);
            } else {
              console.log('não entrei');
            }

            this.getBooks(this.filterSearch, this.filterTag, 0, this.booksPerPage, false, this.sortField, this.sortDirection);
          }
        }, {
          key: "removeTag",
          value: function removeTag(index) {
            console.log('removeTag');
            this.filterTag.splice(index, 1);
            this.getBooks(this.filterSearch, this.filterTag, 0, this.booksPerPage, false, this.sortField, this.sortDirection);
          }
        }, {
          key: "onScroll",
          value: function onScroll() {
            if (this.bookList.length < this.bookService.totalBooks) {
              this.page++;
              this.getBooks(this.filterSearch, this.filterTag, this.page, this.booksPerPage, true, this.sortField, this.sortDirection);
            }
          }
        }, {
          key: "daysSince",
          value: function daysSince(date) {
            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds

            var today = new Date();
            var lendingDate = new Date(date);
            var days = Math.round(Math.abs((+today - +lendingDate) / oneDay));
            return days;
          }
        }, {
          key: "ngOnDestroy",
          value: function ngOnDestroy() {
            this.userSubscription.unsubscribe();
            this.bookListSubscription.unsubscribe();
            this.routeSubscription.unsubscribe(); //this.search.searchInputUpdated.unsubscribe();
          }
        }]);

        return BookListComponent;
      }();

