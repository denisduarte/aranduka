          }
        }, {
          key: "descendingSort",
          value: function descendingSort(a, b) {
            if (a.date < b.date) {
              return 1;
            }

            if (a.date > b.date) {
              return -1;
            }

            return 0;
          }
        }, {
          key: "sortedComments",
          value: function sortedComments(comments) {
            if (comments) return comments.sort(this.descendingSort);
            return [];
          }
