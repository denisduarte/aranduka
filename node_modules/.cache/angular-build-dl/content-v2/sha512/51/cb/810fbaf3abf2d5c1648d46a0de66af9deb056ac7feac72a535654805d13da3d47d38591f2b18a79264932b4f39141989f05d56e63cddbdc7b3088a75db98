      var ActionsComponent = /*#__PURE__*/function () {
        function ActionsComponent(router, datePipe, bookService, userService) {
          _classCallCheck(this, ActionsComponent);

          this.router = router;
          this.datePipe = datePipe;
          this.bookService = bookService;
          this.userService = userService;
          this.bookActions = [null, null, null, null];
        }

        _createClass(ActionsComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            var _this6 = this;

            this.userSubscription = this.userService.user.subscribe(function (user) {
              if (user) {
                _this6.user = user;

                _this6.defineActions(_this6.user, _this6.book);
              }
            });
          }
        }, {
          key: "defineActions",
          value: function defineActions(user, book) {
            var option_BookAvailable = new Action(null, 'fa-check-circle', 'Disponível', 'icon-green');
            var option_EnterWaitingList = new Action(this.onEnterWaitingList.bind(this), 'fa-list-ol', 'Entrar na fila de espera', '');
            var option_LeaveWaitingList = new Action(this.onLeaveWaitingList.bind(this), 'fa-list-ol', 'Sair da fila de espera', '');
            var option_Comments = new Action(this.onOpenComments.bind(this), 'fa-comments', 'Comentários', '');
            var option_AddToWishlist = new Action(this.onAddToWishist.bind(this), 'fa-clipboard-list', 'Adicionar à minha lista', '');
            var option_RemoveFromWishlist = new Action(this.onRemoveFromWishist.bind(this), 'fa-trash-alt', 'Remover da minha lista', '');
            var option_RenewLending = new Action(this.onRenewBook.bind(this), 'fa-undo-alt', 'Renovar empréstimo', '');
            var option_Due = new Action(null, 'fa-exclamation-triangle', 'Devolução do livro atrasada', 'icon-red');
            var option_BorrowBook = new Action(this.onBorrowBook.bind(this), 'fa-book', 'Solicitar empréstimo', '');
            var option_ReturnBook = new Action(this.onReturnBook.bind(this), 'fa-file-signature', 'Devolver livro', '');
            var option_CannotLendBookMax = new Action(null, 'fa-clone', 'Máximo de 2 livros de cada vez', '');
            var option_CannotLendBookWait = new Action(null, 'fa-clock', 'Alguém está na fila de espera', '');
            var option_BookUnavailableQueue = new Action(null, 'fa-times-circle', 'Indisponível. Alguém está na fila de espera.', 'icon-red');
            var option_DueDate = null;
            var option_BookUnavailableUntil = new Action(null, 'fa-times-circle', 'Indisponível ', 'icon-red');

            if (book.lending.length > 0) {
              option_BookUnavailableUntil.text = option_BookUnavailableUntil.text.concat(' até ' + this.datePipe.transform(new Date(book.lending[0].lendingEntry.dueDate), 'dd/MM/yyyy'));
              option_DueDate = new Action(null, 'fa-calendar-alt', 'Devolver em ' + this.datePipe.transform(new Date(book.lending[0].lendingEntry.dueDate), 'dd/MM/yyyy'), '');
            }

            var bookIsAvailable = this.isBookAvailable(book);
            var userCanBorrow = this.canUserBorrow(user);
            var userIsBorrowingBook = this.isUserBorrowingBook(book, user);
            var userIsWaitingBook = this.isUserWaitingBook(book, user);
            var bookIsInUserWishlist = this.isBookInUserWishlist(book, user);
            var bookIsDue = this.isBookDue(book);
            var anotherUserIsWaitingBook = this.isAnotherUserWaitingBook(book, user);
            /*if (book._id == 1 || book._id == 47 ) {
              console.log('bookIsAvailable', bookIsAvailable);
              console.log('anotherUserIsWaitingBook', anotherUserIsWaitingBook);
              console.log('userIsBorrowingBook', userIsBorrowingBook);
              console.log('bookIsDue', bookIsDue);
                 }*/
            // FIRST ACTION

            if (bookIsAvailable && !anotherUserIsWaitingBook) {
              this.bookActions[0] = option_BookAvailable;
            }

            if (bookIsAvailable && anotherUserIsWaitingBook) {
              this.bookActions[0] = option_BookUnavailableQueue;
            }

            if (!bookIsAvailable && !userIsBorrowingBook) {
              this.bookActions[0] = option_BookUnavailableUntil;
            }

            if (!bookIsAvailable && userIsBorrowingBook && !bookIsDue) {
              this.bookActions[0] = option_DueDate;
            }

            if (!bookIsAvailable && userIsBorrowingBook && bookIsDue && anotherUserIsWaitingBook) {
              this.bookActions[0] = option_Due;
            }

            if (!bookIsAvailable && userIsBorrowingBook && bookIsDue && !anotherUserIsWaitingBook) {
              this.bookActions[0] = option_RenewLending;
            } // SECOND ACTION


            if (bookIsAvailable && userCanBorrow && !anotherUserIsWaitingBook) {
              this.bookActions[1] = option_BorrowBook;
            }

            if (bookIsAvailable && userCanBorrow && anotherUserIsWaitingBook && !userIsWaitingBook) {
              //this.bookActions[1] = option_CannotLendBookWait;
              this.bookActions[1] = option_EnterWaitingList;
            }

            if (bookIsAvailable && userCanBorrow && anotherUserIsWaitingBook && userIsWaitingBook) {
              //this.bookActions[1] = option_CannotLendBookWait;
              this.bookActions[1] = option_LeaveWaitingList;
            }

            if (bookIsAvailable && !userCanBorrow) {
              this.bookActions[1] = option_CannotLendBookMax;
            }

            if (!bookIsAvailable && userIsBorrowingBook) {
              this.bookActions[1] = option_ReturnBook;
            }

            if (!bookIsAvailable && !userIsBorrowingBook && !userIsWaitingBook) {
              this.bookActions[1] = option_EnterWaitingList;
            }

            if (!bookIsAvailable && !userIsBorrowingBook && userIsWaitingBook) {
              this.bookActions[1] = option_LeaveWaitingList;
            } // Third ACTION


            if (bookIsInUserWishlist) {
              this.bookActions[2] = option_RemoveFromWishlist;
            }

            if (!bookIsInUserWishlist) {
              this.bookActions[2] = option_AddToWishlist;
            } // Fourth ACTION


            this.bookActions[3] = option_Comments;
          }
        }, {
          key: "isBookAvailable",
          value: function isBookAvailable(book) {
            return book.lending.length ? false : true;
          }
        }, {
          key: "canUserBorrow",
          value: function canUserBorrow(user) {
            return user.borrowing.length < 2;
          }
        }, {
          key: "isUserBorrowingBook",
          value: function isUserBorrowingBook(book, user) {
            if (book.lending.length === 0) {
              return false;
            } else {
              return book.lending[0]._id === user._id;
            }
          }
        }, {
          key: "isUserWaitingBook",
          value: function isUserWaitingBook(book, user) {
