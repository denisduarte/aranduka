      var LendingsComponent = /*#__PURE__*/function () {
        function LendingsComponent(adminService) {
          _classCallCheck(this, LendingsComponent);

          this.adminService = adminService;
          this.lendings = [];
        }

        _createClass(LendingsComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            console.log('LendingsComponent');
            this.loadLendings();
          }
        }, {
          key: "loadLendings",
          value: function loadLendings() {
            var _this37 = this;

            this.adminService.getLendings().subscribe(function (data) {
              _this37.lendings = data;
            }, function (err) {
              console.log(err);
            });
          }
        }, {
          key: "onReturnBook",
          value: function onReturnBook(bookID) {
            this.adminService.returnBook(bookID);
            var index = this.lendings.findIndex(function (book) {
              return book._id == bookID;
            });

            if (index > -1) {
              this.lendings.splice(index, 1);
            }

            return true;
          }
        }, {
          key: "daysSince",
          value: function daysSince(date) {
            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds

            var today = new Date();
            var lendingDate = new Date(date);
            var days = Math.round(Math.abs((+today - +lendingDate) / oneDay));
            return days;
          }
        }]);

        return LendingsComponent;
      }();

