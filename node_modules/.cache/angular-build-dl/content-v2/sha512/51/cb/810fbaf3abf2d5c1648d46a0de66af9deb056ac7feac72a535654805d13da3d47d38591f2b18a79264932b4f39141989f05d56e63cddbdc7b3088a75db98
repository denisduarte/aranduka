        function ShortnamePipe() {
          _classCallCheck(this, ShortnamePipe);
        }

        _createClass(ShortnamePipe, [{
          key: "transform",
          value: function transform(value) {
            var names = value.split(' ');
            var shortenedName = '';

            if (names.length > 1) {
              shortenedName = names[0] + ' ' + names[names.length - 1];
            } else {
              shortenedName = names[0];
            }

            if (shortenedName.length > 20) {
              shortenedName = shortenedName.slice(0, 17) + '...';
            }

            return shortenedName;
          }
        }]);

        return ShortnamePipe;
      }();

